<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kr.ac.konkuk.alldaytimer"
    android:versionCode="1"
    android:versionName="1.0">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".recordActivity"></activity>
        <activity android:name=".EndOfDay" />

        <service
            android:name=".MusicService"
            android:enabled="true" />
        <service
            android:name=".MusicService2"
            android:enabled="true" />

        <activity android:name=".Image2" />
        <activity android:name=".Image" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RightActivity"
            android:label="RightActivity" />
        <activity
            android:name=".LeftActivity"
            android:label="LeftActivity" />
    </application>

</manifest>